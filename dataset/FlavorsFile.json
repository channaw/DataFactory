{
	"name": "FlavorsFile",
	"properties": {
		"linkedServiceName": {
			"referenceName": "FDCStorage",
			"type": "LinkedServiceReference"
		},
		"type": "AzureBlob",
		"structure": [
			{
				"name": "FlavorId",
				"type": "String"
			},
			{
				"name": "FlavorName",
				"type": "String"
			},
			{
				"name": "Image",
				"type": "String"
			},
			{
				"name": "Thumbnail",
				"type": "String"
			}
		],
		"typeProperties": {
			"format": {
				"type": "TextFormat",
				"columnDelimiter": ",",
				"rowDelimiter": "",
